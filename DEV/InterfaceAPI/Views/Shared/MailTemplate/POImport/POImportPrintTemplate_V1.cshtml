                                            <td align="left" valign="top" height="15" style="font-size:16px; color:#222; line-height:25px;">@String.Format("{0:dd-MMM-yyyy}", item.EstimatedDelivery)</td>
                                        </tr>
                                        i = i + 1;
                                        total = total + Convert.ToDecimal(item.LineTotal);
                                    }
                                }
                               
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff; border-top:1px solid #222;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff;border-top:1px solid #222; font-size:16px; color:#222; line-height:40px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">SubTotal</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff;border-top:1px solid #222; border-right:1px solid #fff; padding-right:15px;">@String.Format("{0:n2}", total)</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">TotalTax</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">0</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">TotalFreight</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">0</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">Total PO Amount</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">@String.Format("{0:n2}", total)</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">Currency</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">@Model.Currency</td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>       
    </table>

</body>
</html>

