@model Client.Models.NewLaborScheduling.NewLaborSchedulingVM
@{
    ViewBag.Title = "Somax | Labor Scheduling";
}
@Styles.Render("~/Content/dashboardStyle")
@Styles.Render("~/Content/datatable")
<link href="~/Content/sidebar_custom.css" rel="stylesheet" />
<link href="~/Scripts/Metronic/assets/demo/default/base/GlobalSearchGrid.css" rel="stylesheet" />
@Styles.Render("~/Content/daterangepickercustom")

<div id="newlaborscheduling">
    @if (ViewBag.IsListPage == true)
    {
        @section scripts {
            @Scripts.Render("~/bundles/datatable")
            @Scripts.Render("~/bundles/LaborSchedulingList")
        }
        @Styles.Render("~/Content/LaborSchedulingList")
        @Html.Partial("_NewLaborSchedulingListSearch", Model)
    }
    else
    {
        @section scripts{
            @Scripts.Render("~/bundles/FullCalendar")
            @Scripts.Render("~/bundles/datatable")
            @Scripts.Render("~/bundles/LaborSchedulingCalendar")
        }
        @Styles.Render("~/Content/FullCalendar")
        @Styles.Render("~/Content/LaborSchedulingCalendar")
        @Html.Partial("_NewLaborSchedulingCalendarSearch", Model)
    }
</div>
<script type="text/javascript">
    var IsListPage = '@ViewBag.IsListPage';
</script>