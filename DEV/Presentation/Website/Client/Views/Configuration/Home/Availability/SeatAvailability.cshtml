                                <div></div>
                                <div></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-xl-5 col-md-6 complete">
            <div class="m-portlet m-portlet--fit">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <h3 class="m-portlet__head-text">
                                @Model.Loc.Where(a => a.ResourceId == "spnActiveUsers").FirstOrDefault().Value.ToString()
                            </h3>
                        </div>
                    </div>
                </div>

                <div class="m-portlet__body">
                    <div class="tab-content">
                        <div class="chartBox">
                            <span id="activeUserCount"></span>
                            <div class="lds-ellipsis lds-adjust available-user-loader">
                                <div></div>
                                <div></div>
                                <div></div>
                                <div></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-xl-5 col-md-6 breakIn">
            <div class="m-portlet m-portlet--fit">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
