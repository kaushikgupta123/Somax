    <li>
        <span class="label" id="spnMeter1Units">@Html.LabelFor(m => m.FleetAssetModel.Meter1Units)</span>
        <span class="inputText">
            @Html.ValidationMessageFor(m => m.FleetAssetModel.Meter1Units, "", new { style = "display:none" })
