@model Client.Models.FleetAsset.FleetAssetVM
<div id="changeFleetAssetIDModalDetailsPage" class="modal fade" role="dialog" style="padding-right:0px !important;">
    <div class="modal-dialog modal-info">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">@Model.Loc.Where(a => a.ResourceId == "spnChangeFleetAssetID").FirstOrDefault().Value.ToString() </h4>
            </div>

            <div class="modal-body">

                <div class="form-group">
                    <label for="Line" id="lblLine">@Model.Loc.Where(a => a.ResourceId == "spnFleetAssetID").FirstOrDefault().Value.ToString() :<small>*</small></label>
                    @Html.TextBoxFor(m => m._ChangeFleetAssetIDModel.ClientLookupId, new { @class = "form-control search", autocomplete = "off", id = "txtEquipmentId" })
                </div>
            </div>
            <div class="modal-footer" style="padding:15px 25px !important;">
                <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-close faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalCancel").FirstOrDefault().Value.ToString()</button>
                <button type="button" class="btn btn-blue" style="float:right;" id="changeFleetAsset" data-eqid="@Model._ChangeFleetAssetIDModel.EquipmentId"><i class="fa fa-save faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalSave").FirstOrDefault().Value.ToString()</button>
                <div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>
@Html.HiddenFor(m => m._ChangeFleetAssetIDModel.ClientLookupId, new { id = "hiddenclientlookupid" })
<style type="text/css">
    .ui-tooltip {
        z-index: 1151 !important;
    }

    .errormessage {
        margin-bottom: 1px !important;
    }
</style>
