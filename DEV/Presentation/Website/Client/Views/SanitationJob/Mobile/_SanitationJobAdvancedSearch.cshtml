                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnGlobalAssetLocation").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "AssetLocation"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnStatus").FirstOrDefault().Value.ToString()
                                    @Html.DropDownListFor(m => m.sanitationJobSearchModel.Status, Model.sanitationJobSearchModel.StatusList, "--Select--", new
                               {
                                   @class = "form-control search adv-item mobiscrollselect",
                                   @style = "width:100%;",
                                   @mbsc_dropdown = "true",
                                   @data_label = Model.Loc.Where(a => a.ResourceId == "spnStatus").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnStatus").FirstOrDefault().Value.ToString(),
                                   id = "Status"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnShift").FirstOrDefault().Value.ToString()
                                    @Html.DropDownListFor(m => m.sanitationJobSearchModel.Shift, Model.sanitationJobSearchModel.ShiftList, "--Select--", new
                               {
                                   @class = "form-control search adv-item mobiscrollselect",
                                   @style = "width:100%;",
                                   @mbsc_dropdown = "true",
                                   @data_label = Model.Loc.Where(a => a.ResourceId == "spnShift").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnShift").FirstOrDefault().Value.ToString(),
                                   id = "Shift"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.sanitationJobModel.AssetGroup1Name
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.AssetGroup1_ClientLookUpId, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = @Model.sanitationJobModel.AssetGroup1Name,
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "AssetGroup1ClientLookUpId"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.sanitationJobModel.AssetGroup2Name
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.AssetGroup2_ClientLookUpId, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = @Model.sanitationJobModel.AssetGroup2Name,
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "AssetGroup2ClientLookUpId"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.sanitationJobModel.AssetGroup3Name
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.AssetGroup3_ClientLookUpId, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = @Model.sanitationJobModel.AssetGroup3Name,
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "AssetGroup3ClientLookUpId"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobModel.CreateDate, "{0:MM/dd/yyyy}", new { @class = "form-control search mobidtpicker adv-item", @mbsc_input = "true", @data_label_style = "floating", @placeholder = Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString(), @data_input_style = "box", autocomplete = "off", id = "Created" })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnCreateBy").FirstOrDefault().Value.ToString()
                                    @Html.DropDownListFor(m => m.sanitationJobSearchModel.CreateBy, Model.sanitationJobSearchModel.CreateByList, "--Select--", new
                               {
                                   @class = "form-control search adv-item mobiscrollselect",
                                   @style = "width:100%;",
                                   @mbsc_dropdown = "true",
                                   @data_label = Model.Loc.Where(a => a.ResourceId == "spnCreateBy").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnCreateBy").FirstOrDefault().Value.ToString(),
                                   id = "CreateBy"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnAssigned").FirstOrDefault().Value.ToString()
                                    @Html.DropDownListFor(m => m.sanitationJobSearchModel.Assigned, Model.sanitationJobSearchModel.AssignedList, "--Select--", new
                               {
                                   @class = "form-control search adv-item mobiscrollselect",
                                   @style = "width:100%;",
                                   @mbsc_dropdown = "true",
                                   @data_label = Model.Loc.Where(a => a.ResourceId == "spnAssigned").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnAssigned").FirstOrDefault().Value.ToString(),
                                   id = "Assigned"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnCompleteDate").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.CompleteDate, "{0:MM/dd/yyyy}", new { @class = "form-control search mobidtpicker adv-item", @mbsc_input = "true", @data_label_style = "floating", @placeholder = @Model.Loc.Where(a => a.ResourceId == "spnCompleteDate").FirstOrDefault().Value.ToString(), @data_input_style = "box", autocomplete = "off", id = "CompleteDate" })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnVeryfiedBy").FirstOrDefault().Value.ToString()
                                    @Html.DropDownListFor(m => m.sanitationJobSearchModel.VerifiedBy, Model.sanitationJobSearchModel.VerifiedByList, "--Select--", new
                               {
                                   @class = "form-control search adv-item mobiscrollselect",
                                   @style = "width:100%;",
                                   @mbsc_dropdown = "true",
                                   @data_label = Model.Loc.Where(a => a.ResourceId == "spnVeryfiedBy").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnVeryfiedBy").FirstOrDefault().Value.ToString(),
