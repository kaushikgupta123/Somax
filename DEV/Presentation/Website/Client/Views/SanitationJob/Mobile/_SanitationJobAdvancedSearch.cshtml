                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnVeryfiedDate").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.VerifiedDate,
                                   "{0:MM/dd/yyyy}", new { @class = "form-control search mobidtpicker adv-item", @mbsc_input = "true", @data_label_style = "floating", @placeholder = Model.Loc.Where(a => a.ResourceId == "spnVeryfiedDate").FirstOrDefault().Value.ToString(), @data_input_style = "box", autocomplete = "off", id = "VeryfiedDate" })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                @if(Model.JobDetailsModel.ExternalSanitation == true)
                                {
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnExtracted").FirstOrDefault().Value.ToString()
                                        @Html.DropDownListFor(m => m.sanitationJobSearchModel.Extracted, Model.sanitationJobSearchModel.SanitExtractedList, "--Select--", new
                                   {
                                       @class = "form-control search adv-item mobiscrollselect",
                                       @style = "width:100%;",
                                       @mbsc_dropdown = "true",
                                       @data_label = Model.Loc.Where(a => a.ResourceId == "spnExtracted").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnExtracted").FirstOrDefault().Value.ToString(),
                                       id = "Extracted"
                                   })
                                    </label>
                                
                                }
                                else
                                {
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnScheduledDate").FirstOrDefault().Value.ToString()
                                        @Html.TextBoxFor(m => m.sanitationJobSearchModel.ScheduledDate, "{0:MM/dd/yyyy}", new { @class = "form-control search mobidtpicker adv-item", @mbsc_input = "true", @data_label_style = "floating", @placeholder = Model.Loc.Where(a => a.ResourceId == "spnScheduledDate").FirstOrDefault().Value.ToString(), @data_input_style = "box", autocomplete = "off",id = "ScheduledDate" })
                                    </label>
                                }
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="slide-modal-footer"></div>
</div>
<style>
    #sidebar.slide-modal.slide-active {
        width: 350px !important;
        left: auto !important;
        right: 0 !important;
    }

    .slide-back-btn.right {
        left: auto;
        right: 12px;
    }
</style>