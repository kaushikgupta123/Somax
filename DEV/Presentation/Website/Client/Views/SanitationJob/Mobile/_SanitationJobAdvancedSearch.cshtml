@model Client.Models.Sanitation.SanitationVM
<div class="slide-modal" id="sidebar">
    <div class="slide-modal-head">
        <a class="slide-back-btn SanitationJobDataAdvSrchclearstate"> <i class="fa fa-angle-left"></i> Back</a>
        <div class="slide-back-title">
            @Model.Loc.Where(a => a.ResourceId == "globalFilter").FirstOrDefault().Value.ToString()
            <span class="filteritemcount">0</span>
        </div>
        <a id="btnSanitationJobDataAdvSrch" class="slide-back-btn right"> @Model.Loc.Where(a => a.ResourceId == "GlobalApply").FirstOrDefault().Value.ToString()</a>

    </div>
    <div class="slide-modal-body">
        <div mbsc-page class="demo-responsive">
            <div style="height:100%">
                <div mbsc-form class="mbsc-form-grid">
                    <div class="mbsc-grid">
                        <div class="mbsc-row" id="advsearchsidebar">
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnJobID").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.ClientLookupId, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnJobID").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "JobId"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.Description, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "Description"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "GlobalChargeTo").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.ChargeTo_ClientLookupId, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "GlobalChargeTo").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "ChargeTo"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnChargeToName").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.ChargeTo_Name, new
                               {
                                   @class = "form-control search adv-item",
                                   @mbsc_input = "true",
                                   @data_label_style = "floating",
                                   @placeholder = Model.Loc.Where(a => a.ResourceId == "spnChargeToName").FirstOrDefault().Value.ToString(),
                                   @data_input_style = "box",
                                   autocomplete = "off",
                                   id = "ChargeToName"
                               })
                                </label>
                            </div>
                            <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                <label>
                                    @Model.Loc.Where(a => a.ResourceId == "spnGlobalAssetLocation").FirstOrDefault().Value.ToString()
                                    @Html.TextBoxFor(m => m.sanitationJobSearchModel.AssetLocation, new
                               {
