@model Client.Models.Sanitation.SanitationVM
<style>
    .inputText.wthAdjstNew2 {
        width: 67.5% !important;
    }

    ul.listView li input.search {
        border-radius: 0.25rem 0 0 0.25rem;
        margin-top: 2px;
    }

    .TreeAdjustNew {
        right: -2px !important;
    }
    .takePic {
        margin: 0 auto;
        max-width: 300px;
        text-align: center;
        padding: 0 0 15px 0;
    }

    .col-sec-row {
        display:flex;
    }

    .col-sec-row span.label {
        width:250px;
    }

    .col-sec-row .right-col {
        width: calc(100% - 250px) !important;
        display: flex;
    }

    .col-sec-row .right-col .inputText.wthAdjstNew2 {
        width: calc(100% - 50px) !important;
    }

    .col-sec-row .right-col .chargeTree.TreeAdjustNew {
        width: 35px !important;
        line-height: 35px;
        position: relative !important;
        text-align: center;
        right: 0px !important;
        top: 0px !important;
        margin-left: 10px;
    }


    .col-sec-row-normal {
        display: flex;
    }

    .col-sec-row-normal span.label {
        width: 250px;
    }
    .col-sec-row-normal .right-col .textarea-box {
        width: 100%;
    }
    .col-sec-row-normal .right-col {
        width: calc(100% - 250px) !important;
        display: flex;
    }

    @@media(max-width:900px) {
        .col-sec-row .right-col {
            width: calc(100% - 200px) !important;
        }

        .col-sec-row-normal .right-col {
            width: calc(100% - 200px) !important;
        }
    }
    @@media(max-width:700px) {
        .col-sec-row .right-col .inputText.wthAdjstNew2 {
            width: calc(100% - 0px) !important;
        }
        .col-sec-row-normal .right-col {
            width: 100% !important;
        }
        .col-sec-row .right-col .chargeTree.TreeAdjustNew {
            width: calc(100% - 0px) !important;
            margin: 10px 0 0 0;
            background-color:#fff;
            border: #b9c1c4 1px solid !important;
            border-radius:5px;
        }
        .right-col {
            display:block;
        }
        .col-sec-row .right-col {
            display: block;
            width: 100% !important;
        }
    }

</style>
@Styles.Render("~/Content/datatable");
