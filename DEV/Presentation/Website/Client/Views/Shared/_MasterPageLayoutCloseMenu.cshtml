@using DataContracts
@using Client.Models.Common
@{
    bool PurchaseOrderImportInUse = false;
    bool OraclePurchaseOrderImportInUse = false;
    bool OracleVendorMasterImportInUse = false;
    bool PurchaseOrderImportSwitch1 = false;
    var menulist = (List<ViewModels.MenuVM>)Session["MenuDetails"];
    var userdata = Session["userData"];
    UserData userData = new UserData();
    userData = (UserData)userdata;
    var InterfacePropData = (List<InterfacePropModel>)Session["InterfacePropData"];
    if (InterfacePropData != null && InterfacePropData.Count > 0)
    {
        PurchaseOrderImportInUse = InterfacePropData.Where(x => x.InterfaceType == InterfacePropConstants.PurchaseOrderImport).Select(x => x.InUse).FirstOrDefault();
        PurchaseOrderImportSwitch1 = InterfacePropData.Where(x => x.InterfaceType == InterfacePropConstants.PurchaseOrderImport).Select(x => x.Switch1).FirstOrDefault();
        OraclePurchaseOrderImportInUse = InterfacePropData.Where(x => x.InterfaceType == ApiConstants.OraclePOImport).Select(x => x.InUse).FirstOrDefault();
        OracleVendorMasterImportInUse = InterfacePropData.Where(x => x.InterfaceType == ApiConstants.OracleVendorMasterImport).Select(x => x.InUse).FirstOrDefault();
    }

    var encKeyV = System.Configuration.ConfigurationManager.AppSettings["encKeyV"].ToString();
    string userNameIntials = ((userData.DatabaseKey.User.FirstName != null && userData.DatabaseKey.User.FirstName.Length > 0) ? userData.DatabaseKey.User.FirstName.Trim()[0].ToString() : "")
                              + ((userData.DatabaseKey.User.LastName != null && userData.DatabaseKey.User.LastName.Length > 0) ? userData.DatabaseKey.User.LastName.Trim()[0].ToString() : "");
}

