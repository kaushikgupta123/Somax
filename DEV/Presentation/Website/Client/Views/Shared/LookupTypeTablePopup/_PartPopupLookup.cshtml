
<div id="partModal" class="modal fade dropdown-modal" role="dialog" style="display:none;">
    <div class="modal-dialog partPopup">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" id="popPartTable">@UtilityFunction.GetMessageFromResource("spnSelectPart", LocalizeResourceSetConstants.Global)</h4>
            </div>
            <div class="modal-body">
                <div id="tbldropdown" class="dropdown-content tbl-dropdown" style="display:block;">
                    <div class="table-responsive dropTableOuter">
                        <table id="XrefPartsTable" class="stripe dropTable globalDropTable" style="width:100%;">
                            <thead>
                                <tr>
                                    <th>@UtilityFunction.GetMessageFromResource("spnPartID", LocalizeResourceSetConstants.Global)</th>
                                    <th>@UtilityFunction.GetMessageFromResource("spnDescription", LocalizeResourceSetConstants.Global)</th>
                                    <th>@UtilityFunction.GetMessageFromResource("spnUPCCode", LocalizeResourceSetConstants.Global)</th>
                                    <th>@UtilityFunction.GetMessageFromResource("spnManufacturer", LocalizeResourceSetConstants.Global)</th>
                                    <th>@UtilityFunction.GetMessageFromResource("spnManufacturerID", LocalizeResourceSetConstants.Global)</th>
                                    <th>@UtilityFunction.GetMessageFromResource("spnStockType", LocalizeResourceSetConstants.Global)</th>

                                </tr>
                            </thead>
                            <tfoot id="tblpartfooter" style="display:none">
                                <tr>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnPartID", LocalizeResourceSetConstants.Global)</th>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnDescription", LocalizeResourceSetConstants.Global)</th>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnUPCCode", LocalizeResourceSetConstants.Global)</th>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnManufacturer", LocalizeResourceSetConstants.Global)</th>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnManufacturerID", LocalizeResourceSetConstants.Global)</th>
                                    <th style="position:relative;">@UtilityFunction.GetMessageFromResource("spnStockType", LocalizeResourceSetConstants.Global)</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer" style="padding:15px 25px !important;">
                <div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>
