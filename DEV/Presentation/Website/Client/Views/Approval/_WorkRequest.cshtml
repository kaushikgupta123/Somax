@model Client.Models.Approval.ApprovalVM

<div class="row">
    <div class="col-xl-12">
        <div class="m-portlet" id="m_portlet" style="padding:15px;">
            <div class="actionBar" style="display:block" id="workRequestGridAction">
                <div class="pull-right">
                    <button type="button" class="btn btn-white import-export" id="btnWorkRequestExport" disabled onclick="funcShowExportbtn()">@Model.Loc.Where(a => a.ResourceId == "spnExport").FirstOrDefault().Value.ToString()</button>
                    <div style="clear:both;"></div>
                </div>
                <div style="clear:both;"></div>
            </div>

            <div style="clear:both;"></div>
            <div id="WorkRequestListid" class="tabcontentRL">
                <div class="table-responsive">
                    <table id="workRequestSearchTable" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                        <thead>
                            <tr>
                                <th data-th-index="0">@Model.Loc.Where(a => a.ResourceId == "spnWorkRequest").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="1">@Model.Loc.Where(a => a.ResourceId == "GlobalChargeTo").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="2" style="text-align:right !important;">@Model.Loc.Where(a => a.ResourceId == "spnChargeToName").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="3" style="text-align:right !important;">@Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="4" style="text-align:right !important;">@Model.Loc.Where(a => a.ResourceId == "GlobalType").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="5">@Model.Loc.Where(a => a.ResourceId == "GlobalDate").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="6">@Model.Loc.Where(a => a.ResourceId == "spnLogComment").FirstOrDefault().Value.ToString()</th>
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="bttn_pagin">
                </div>
            </div>

        </div>
    </div>

</div>
@Html.Partial("_Export")
<div class="row workRequestView">
</div>




