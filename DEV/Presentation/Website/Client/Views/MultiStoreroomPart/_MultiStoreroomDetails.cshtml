@model Client.Models.MultiStoreroomPart.MultiStoreroomPartVM
@{
    string imageUrl = Model.MultiStoreroomPartModel.PartImageUrl;
    if (Model.MultiStoreroomPartModel.ClientOnPremise)
    {
        imageUrl = UtilityFunction.PhotoBase64ImgSrc(Model.MultiStoreroomPartModel.PartImageUrl);
    }
}

@Styles.Render("~/Content/somaxcommentsection")

<div class="m-subheader fixedDiv1">
    <div class="new-m-subheader">
        <div class="filterDescription">
            <a href="javascript:void(0)" id="linkToSearch">
                <span id="spnlinkToSearch"></span>
            </a>
            @if (Model.IsAddPartFromEquipment)
            {
                <b>-</b>
                <a href="javascript:void(0)" id="linkToEquipment">
                    <span id="spnlinkToEquipment"></span>
                    @Model.Equipment_ClientLookupId
                </a>
            }
            - @Model.MultiStoreroomPartModel.ClientLookupId
        </div>

        <div class="ActionBttnArea">
            @{
                var changePartIdSecurity = (Model.security.Parts.ChangeClientLookupId
                                            && !Model._userdata.Site.UsePartMaster
                                            && !Model._userdata.Site.NoPartIdChange)
                                        || (Model.security.Parts.ChangeClientLookupId
                                            && Model._userdata.DatabaseKey.Personnel.InitialLevel == "ChangePartId");


            }
            @if (Model.security.Parts.Edit)
            {
                <div class="actionDiv pull-left" id="ad1">
                    <div class="btn-group bootstrap-select m-datatable__pager-size dropup show selectDrop actionNewDrop">
                        <div class="btn-group bootstrap-select m-datatable__pager-size" style="width:100%;">
                            <button type="button" class="btn dropdown-toggle bs-placeholder btn-default" data-toggle="dropdown" role="button" title=""><span class="filter-option pull-left">@Model.Loc.Where(a => a.ResourceId == "globalAction").FirstOrDefault().Value.ToString()</span>&nbsp;<span class="bs-caret"><span class="caret"></span></span></button>
                            <div class="dropdown-menu open" role="combobox">
                                <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                    @if (changePartIdSecurity)
                                    {
                                        <li data-toggle="modal" id="optchangemultistoreroompartid"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnChangePartId").FirstOrDefault().Value.ToString()</a></li>
                                    }
                                    @*V2-1203*@
                                    @if (Model.security.Parts.Model)
                                    {
                                        <li id="addMultiStoreroomPartModel" data-id=@Model.MultiStoreroomPartModel.PartId><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnModel").FirstOrDefault().Value.ToString()</a></li>
                                    }
                                    @if (Model.security.Parts.Delete && Model.MultiStoreroomPartModel.InactiveFlag == true)
                                    {
                                        <li id="actinctivatePartStoreroom"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnGlobalActivate").FirstOrDefault().Value.ToString()</a></li>
                                    }
                                    @if (Model.security.Parts.Delete && Model.MultiStoreroomPartModel.InactiveFlag == false)
                                    {
                                        <li id="actinctivatePartStoreroom"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnGlobalInactivate").FirstOrDefault().Value.ToString()</a></li>
                                    }
                                    @if (Model.security.Parts_Photos.Create)
                                    {
                                        <li id="liAddimage">
                                            <div class="bttns" style="margin-left:15px">
                                                <label for="files" class=""><i class=""></i>@Model.Loc.Where(a => a.ResourceId == "spnAddPhotos").FirstOrDefault().Value.ToString()</label>
                                                <input id="files" accept=".jpeg,.jpg,.png,.JPEG,.JPG,.PNG" style="visibility:hidden;" type="file" value="">
                                            </div>
                                        </li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            }
            else if (Model.security.Parts_Photos.Create)
            {
                <div class="actionDiv pull-left addphotofrommobile" id="ad1" style="margin-right:0;">
                    <div class="btn-group bootstrap-select m-datatable__pager-size dropup show selectDrop actionNewDrop">
                        <div class="btn-group bootstrap-select m-datatable__pager-size" style="width:100%;">
                            <button type="button" class="btn dropdown-toggle bs-placeholder btn-default" data-toggle="dropdown" role="button" title=""><span class="filter-option pull-left">@Model.Loc.Where(a => a.ResourceId == "globalAction").FirstOrDefault().Value.ToString()</span>&nbsp;<span class="bs-caret"><span class="caret"></span></span></button>
                            <div class="dropdown-menu open" role="combobox">
                                <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                    <li id="liAddimage">
                                        <div class="bttns" style="margin-left:15px">
                                            <label for="files" class=""><i class=""></i>@Model.Loc.Where(a => a.ResourceId == "spnAddPhotos").FirstOrDefault().Value.ToString()</label>
                                            <input id="files" accept=".jpeg,.jpg,.png,.JPEG,.JPG,.PNG" style="visibility:hidden;" type="file" value="">
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            }

            @if (Model.security.Parts.Create)
            {
                <button type="button" class="btn newBtn-add addparts innerBtn pull-left" id="AddPart" title="Add Part"><i class="fa fa-plus"></i></button>
            }
            <div style="clear:both;"></div>
        </div>
        <div style="clear:both;"></div>
    </div>
</div>
