@model Client.Models.Work_Order.WorkOrderVM
<div id="WODateRangeModalForCreateDate" class="modal fade" role="dialog">
    <div class="modal-dialog modal-info">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Select</h4>
            </div>
           
            <div class="modal-body">
                <div class="form-group">
                    <label for="Line" id="lblLine">@Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString() :</label>
                    <select style="width:100%;" class="form-control search select2picker" id="cmbcreateview">
                        <option value="">--Select--</option>
                        @foreach (var item in Model.workOrderModel.DateRangeDropListForWOCreatedate)
                        {
                            <option value="@item.Value">@item.Text</option>
                        }
                    </select>
                    <div style="padding:10px 0 0 10px; border-left: 1px dashed #b9c1c4;display:none;" id="timeperiodcontainerForCreateDate">
                        <label for="">@Model.Loc.Where(a => a.ResourceId == "spnGlobalTimePeriod").FirstOrDefault().Value.ToString() :</label>
                        <input type="text" class="form-control search adv-item dtrangepicker" id="createdaterange" autocomplete="off">
                    </div>
                </div>
               
            </div>           
            <div class="modal-footer" style="padding:15px 25px !important;">
                <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-close buttonIcon" ></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalCancel").FirstOrDefault().Value.ToString()</button>
                <button type="button" class="btn btn-blue" style="float:right;" id="btntimeperiodForCreateDate"><i class="fa fa-save buttonIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalOK").FirstOrDefault().Value.ToString()</button>
                <div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>