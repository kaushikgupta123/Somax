    }

    .img-radio {
        margin: 10px;
        margin-right: 0;
        margin-bottom: 0;
        font-size: 13px;
        font-weight: 400;
        display: inline-block;
    }

    .radio label {
        cursor: pointer;
    }

    .img-radio input[type=radio] {
        position: absolute;
        opacity: 0;
    }

    .img-radio input[type=radio] + .radio-label:before {
        content: "";
        background: #f4f4f4;
        border-radius: 100%;
        border: 1px solid #767676;
        display: inline-block;
        width: 15px;
        height: 15px;
        position: relative;
        top: 2px;
        margin-right: .5em;
        vertical-align: top;
        cursor: pointer;
        text-align: center;
        transition: all 250ms ease;
    }

    .img-radio input[type=radio]:checked + .radio-label:before {
        background-color: #0075ff;
        border: 1px solid #0075ff;
        box-shadow: inset 0 0 0 4px #f4f4f4;
    }

    .img-radio input[type=radio]:focus + .radio-label:before {
        outline: none;
        border-color: #3197EE;
    }

    .img-radio input[type=radio]:disabled + .radio-label:before {
        box-shadow: inset 0 0 0 4px #f4f4f4;
        border-color: #b4b4b4;
        background: #b4b4b4;
    }

    .img-radio input[type=radio] + .radio-label:empty:before {
        margin-right: 0;
    }

    .app-panel {
