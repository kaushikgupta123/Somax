            </div>
        </div>

        <div class="searchList">
            <ul id="astsearchListul">
                @foreach (var item in Model.InactiveFlagList)
                {
                    <li id="@item.Value" class="astsearchdrpbox"><a href="javascript:void(0)">@item.Text</a></li>
                }
            </ul>
        </div>
    </div>
}



