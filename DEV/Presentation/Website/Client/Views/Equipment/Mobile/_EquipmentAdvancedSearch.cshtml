                                       @mbsc_dropdown = "true",
                                       @data_label = Model.Loc.Where(a => a.ResourceId == "spnAccount").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnAccount").FirstOrDefault().Value.ToString(),
                                       id = "AccountSearchId"
                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnAssetNo").FirstOrDefault().Value.ToString()
                                        @Html.TextBoxFor(m => m.EquipModel.AssetNumber, new
                                   {
                                       @class = "form-control search adv-item",
                                       @mbsc_input = "true",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnName").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       autocomplete = "off",
                                       id = "AssetsNumber"
                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnAvailability").FirstOrDefault().Value.ToString()
                                        @Html.DropDownListFor(m => m.EquipModel.AssetAvailability, Model.LookupAssetAvailability, "--Select--", new
                                   {
                                       @class = "form-control search adv-item mobiscrollselect",
                                       @style = "width:100%;",
                                       @mbsc_dropdown = "true",
                                       @data_label = Model.Loc.Where(a => a.ResourceId == "spnAvailability").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnAvailability").FirstOrDefault().Value.ToString(),
                                       id = "ddlassetAvailability"
                                   })
                                    </label>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="slide-modal-footer"></div>
</div>
<style>
    #sidebar.slide-modal.slide-active {
        left: auto !important;
        right: 0 !important;
    }

    .slide-back-btn.right {
        left: auto;
        right: 12px;
    }
</style>