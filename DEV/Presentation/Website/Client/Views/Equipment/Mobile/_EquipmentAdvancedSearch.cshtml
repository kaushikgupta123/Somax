                                       @mbsc_dropdown = "true",
                                       @data_label = Model.EquipModel.AssetGroup3Name,
                                       @data_input_style = "box",
                                       @data_label_style = "floating",
                                       @placeholder = Model.EquipModel.AssetGroup3Name,
