                                       @mbsc_input = "true",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnName").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       autocomplete = "off",
                                       id = "SerialNumber"
                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "GlobalType").FirstOrDefault().Value.ToString()
                                        @Html.DropDownListFor(m => m.EquipModel.Type, Model.LookupTypeList, "--Select--", new
                                   {
                                       @class = "form-control search adv-item mobiscrollselect",
                                       @style = "width:100%;",
                                       @mbsc_dropdown = "true",
                                       @data_label = Model.Loc.Where(a => a.ResourceId == "GlobalType").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "GlobalType").FirstOrDefault().Value.ToString(),
                                       id = "ddlType"
                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnMake").FirstOrDefault().Value.ToString()
                                        @Html.TextBoxFor(m => m.EquipModel.Make, new
                                   {
                                       @class = "form-control search adv-item",
                                       @mbsc_input = "true",
                                       @data_label_style = "floating",
                                       @placeholder = Model.Loc.Where(a => a.ResourceId == "spnName").FirstOrDefault().Value.ToString(),
                                       @data_input_style = "box",
                                       autocomplete = "off",
                                       id = "Make"
                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnModelNo").FirstOrDefault().Value.ToString()
                                        @Html.TextBoxFor(m => m.EquipModel.ModelNumber, new
                                   {
                                       @class = "form-control search adv-item",
                                       @mbsc_input = "true",
