                                   })
                                    </label>
                                </div>
                                <div class="mbsc-col-12 mbsc-col-md-6 mbsc-col-lg-6">
                                    <label>
                                        @Model.Loc.Where(a => a.ResourceId == "spnSerialNo").FirstOrDefault().Value.ToString()
                                        @Html.TextBoxFor(m => m.EquipModel.SerialNumber, new
