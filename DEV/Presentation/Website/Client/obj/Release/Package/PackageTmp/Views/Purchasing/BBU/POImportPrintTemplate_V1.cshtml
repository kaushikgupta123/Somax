                                    <th width="100" align="left" valign="middle" height="40" scope="col" style="font-size:16px; color:#222; line-height:40px; font-weight:600; border-bottom:1px solid #222;">@Model.Loc.Where(a => a.ResourceId == "spnDueDate").FirstOrDefault().Value.ToString()</th>
                                </tr>
                                @{
                                    int i = 2;
                                    decimal total = 0;
                                    decimal linetot = 0;
                                    foreach (var item in Model.POLineItemList)
                                    {
                                       
                                        <tr>
                                            <td align="right" valign="middle" height="15" style="padding-right:10px; border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@item.LineNumber</td>
                                            <td align="left" valign="middle" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:22px; font-weight:normal;">@item.PartClientLookupId</td>
                                            <td align="left" valign="top" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px; font-weight:normal; padding-left:15px; padding-top:15px;">@item.Description</td>
                                            <td align="left" valign="middle" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@item.PartNumber</td>
                                            <td align="right" valign="middle" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@String.Format("{0:n2}", item.PurchaseQuantity)</td>
                                            <td align="right" valign="middle" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@item.PurchaseUOM</td>
                                            <td align="right" valign="middle" height="15" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@String.Format("{0:n2}", item.PurchaseCost)</td>
                                            <td align="right" valign="middle" height="15" style="padding-right:10px;border-right:1px solid #fff; font-size:16px; color:#222; line-height:40px; font-weight:normal;">@String.Format("{0:n2}", item.LineTotal)</td>
                                            <td align="left" valign="middle" height="15" style="font-size:16px; color:#222; line-height:40px;">@String.Format("{0:dd-MMM-yyyy}", item.EstimatedDelivery)</td>
                                        </tr>
                                        i = i + 1;
                                        total = total + Convert.ToDecimal(item.LineTotal);
                                    }
                                }
                               
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff; border-top:1px solid #222;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff;border-top:1px solid #222; font-size:16px; color:#222; line-height:40px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">@Model.Loc.Where(a => a.ResourceId == "spnSubTotal").FirstOrDefault().Value.ToString()</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff;border-top:1px solid #222; border-right:1px solid #fff; padding-right:15px;">@String.Format("{0:n2}", total)</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">@Model.Loc.Where(a => a.ResourceId == "spnTotalTax").FirstOrDefault().Value.ToString()</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">0</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">@Model.Loc.Where(a => a.ResourceId == "spnTotalFreight").FirstOrDefault().Value.ToString()</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">0</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">@Model.Loc.Where(a => a.ResourceId == "spnTotalPOAmount").FirstOrDefault().Value.ToString()</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">@String.Format("{0:n2}", total)</td>
                                </tr>
                                <tr>
                                    <td height="30" colspan="6" align="center" valign="middle" style="border-right:1px solid #fff;"></td>
                                    <td height="30" colspan="2" align="right" valign="middle" style="border-right:1px solid #fff; font-size:16px; color:#222; line-height:20px;  font-weight:600; border-bottom:1px solid #fff; padding-right:15px;">@Model.Loc.Where(a => a.ResourceId == "spnCurrency").FirstOrDefault().Value.ToString()</td>
                                    <td align="right" valign="middle" height="30" style="font-size:16px; color:#000; line-height:30px; font-weight:500; border-bottom:1px solid #fff; border-right:1px solid #fff; padding-right:15px;">@Model.PurchaseOrderModel.Currency</td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>

        <tr>
          <td align="left" valign="top" height="50"><img src="../Content/images/space.png" alt="" /></td>
        </tr>
    </table>

</body>
</html>

