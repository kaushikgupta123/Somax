@model Client.Models.PartsManagement.PartsManagementRequest.PartsManagementRequestVM

<div class="m-subheader">
    <div class="new-m-subheader">
        <div class="PageTitle">
            <div class="align-items-center separator">
                <h3 class="m-subheader__title ">
                  @Model.Loc.Where(a => a.ResourceId == "spnPartMasterRequestDetail").FirstOrDefault().Value.ToString()  
                </h3>
            </div>
        </div>

        <div class="NewBreadcrumbArea">
            <div class="newBreadcrumb">
                <ul>
                    <li><a href="/Dashboard/Dashboard">&nbsp;</a></li>
                    <li><a href="/PartsManagementRequest/Index?page=Inventory_Parts_Management_Parts_Management_Requests">@Model.Loc.Where(a => a.ResourceId == "spnPartMasterRequestSearch").FirstOrDefault().Value.ToString()</a></li>
                    <li><a href="javascript:void(0)" id="pmrid" data-val="@Model.assignPartMastertoIndusnetBakeryModel.PartMasterRequestId" data-val-rtype="@Model.assignPartMastertoIndusnetBakeryModel.RequestType">@Model.assignPartMastertoIndusnetBakeryModel.PartMasterRequestId</a></li>
                    <li class="active">@Model.Loc.Where(a => a.ResourceId == "globalEdit").FirstOrDefault().Value.ToString()</li>
                </ul>
                <div style="clear:both;"></div>
            </div>
        </div>
        <div style="clear:both;"></div>
    </div>
</div>

@Html.Partial("_ErrorDiv")

@using (Ajax.BeginForm("EditPartManagementRequestForAssign", "PartsManagementRequest", new AjaxOptions { HttpMethod = "Post", OnSuccess = "EditPartMasterRequestSuccess", OnBegin = "AjaxBeginFormBegin", OnComplete = "AjaxBeginFormComplete", OnFailure = "AjaxBeginFormFaillure" }))
{
    @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-xl-12">
            <div class="m-portlet newPort" id="m_portlet" style="padding:15px;">
                <div class="bottomBlock">
                    <div class="tabsArea nobg">
                        <ul class="listView nobrd">
                            <li>
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnRequestId").FirstOrDefault().Value.ToString(): </span>
                                <span class="inputText">
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.PartMasterRequestId, new { @class = "form-control search readonly", @readonly = "readonly", autocomplete = "off" })
                                </span>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnJustification").FirstOrDefault().Value.ToString() : <small>*</small></span>
                                <span class="inputText" style="height:auto;">
                                    @Html.ValidationMessageFor(m => m.assignPartMastertoIndusnetBakeryModel.Justification, "", new { style = "display:none" })
                                    @Html.TextAreaFor(m => m.assignPartMastertoIndusnetBakeryModel.Justification, new { @class = "form-control search", autocomplete = "off" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                            <li>
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnRequestType").FirstOrDefault().Value.ToString(): </span>
                                <span class="inputText">
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.RequestType, new { @class = "form-control search readonly", @readonly = "readonly", autocomplete = "off" })
                                </span>
                            </li>
                            <li>
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnPartMasterNo").FirstOrDefault().Value.ToString(): </span>
                                <span class="inputText">
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.PartMaster_ClientLookupId, new { @class = "form-control search readonly", @readonly = "readonly", autocomplete = "off" })
                                </span>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString(): </span>
                                <span class="inputText" style="height:auto;">
                                    @Html.TextAreaFor(m => m.assignPartMastertoIndusnetBakeryModel.PartMaster_LongDescription, new { @class = "form-control search readonly", @readonly = "readonly", autocomplete = "off" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnUnitCost").FirstOrDefault().Value.ToString() : <small>*</small></span>
                                <span class="inputText" style="height:auto;">
                                    @Html.ValidationMessageFor(m => m.assignPartMastertoIndusnetBakeryModel.UnitCost, "", new { style = "display:none" })
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.UnitCost, new { @class = "form-control search decimalinput", autocomplete = "off" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnLocation").FirstOrDefault().Value.ToString() : <small>*</small></span>
                                <span class="inputText" style="height:auto;">
                                    @Html.ValidationMessageFor(m => m.assignPartMastertoIndusnetBakeryModel.Location, "", new { style = "display:none" })
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.Location, new { @class = "form-control search", maxlength = "31" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "globalMinimumQuantity").FirstOrDefault().Value.ToString() : </span>
                                <span class="inputText" style="height:auto;">
                                    @Html.ValidationMessageFor(m => m.assignPartMastertoIndusnetBakeryModel.QtyMinimum, "", new { style = "display:none" })
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.QtyMinimum, new { @class = "form-control search decimalinput", autocomplete = "off", @id = "QtyMinimum" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                            <li style="height:auto;">
                                <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalMaximumQuantity").FirstOrDefault().Value.ToString() : <small>*</small></span>
                                <span class="inputText" style="height:auto;">
                                    @Html.ValidationMessageFor(m => m.assignPartMastertoIndusnetBakeryModel.QtyMaximum, "", new { style = "display:none" })
                                    @Html.TextBoxFor(m => m.assignPartMastertoIndusnetBakeryModel.QtyMaximum, new { @class = "form-control search decimalinput", autocomplete = "off" })
                                </span>
                                <div style="clear:both;"></div>
                            </li>
                        </ul>
                    </div>
                    <div style="clear:both;"></div>
                    <div class="idBttns">
                        <div class="bttn_pagin">
                            <div class="bttns2">
                                <button type="submit" class="btn btn-blue mobBttn"><i class="fa fa-save faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalSave").FirstOrDefault().Value.ToString()</button>
                            </div>
                            <div class="bttns"><button type="button" class="btn btn-blue mobBttn" id="btnsPartMasterRequestDetailcancelForAssign"><i class="fa fa-close faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalCancel").FirstOrDefault().Value.ToString()</button></div>
                            <div style="clear:both;"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.HiddenFor(m => m.assignPartMastertoIndusnetBakeryModel.PartMasterRequestId)
    @Html.HiddenFor(m => m.assignPartMastertoIndusnetBakeryModel.RequestType)

}




