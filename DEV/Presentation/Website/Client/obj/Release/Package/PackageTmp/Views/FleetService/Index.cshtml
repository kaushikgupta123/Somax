@section scripts {
    @Scripts.Render("~/bundles/datatable")
    @Scripts.Render("~/bundles/imagezoom")
    @Scripts.Render("~/bundles/FleetService")
    @Scripts.Render("~/bundles/SomaxCkEditor")

    <script type="text/javascript">
        com_github_culmat_jsTreeTable.register(this);
        $(document).ready(function () {
            // SetControls();
            $.validator.unobtrusive.parse(document);
            $('.select2picker, form').change(function () {
                var areaddescribedby = $(this).attr('aria-describedby');
                if ($(this).parents().find('.sidebar-content').attr('id') != "advsearchsidebar") {
                    if ($(this).valid()) {
                        if (typeof areaddescribedby !== 'undefined') {
                            $('#' + areaddescribedby).hide();
                        }
                    }
                    else {
                        if (typeof areaddescribedby !== 'undefined') {
                            $('#' + areaddescribedby).show();
                        }
                    }
                }
            });
            $(document).find('.select2picker').select2({});
        })
    </script>
    <script type="text/javascript">
        com_github_culmat_jsTreeTable.register(this);
    </script>
}

    <div id="fleetservicemaincontainer">
        @if (Model.IsRedirectFromAsset == true)
        {
            //code for redirect
        }
        else
        {
            @Html.Partial("_FleetServiceSearch")
        }
        @Html.HiddenFor(m => m.IsRedirectFromAsset, new { id = "IsRedirectFromAsset" })
        @Html.HiddenFor(m => m.SOId, new { id = "SOId" })
    </div>
<style>
