                                                            <td width="323" height="40" style=" border-right: 1px solid #cccccc; border-bottom: 1px solid #cccccc; font-size: 14px; color: #737272; line-height: 40px; font-weight: normal; padding-left: 15px;">@Model.Loc.Where(a => a.ResourceId == "spnAccount").FirstOrDefault().Value.ToString() : @item.AccountClientLookupId </td>
                                                            <td width="323" height="40" style="border-right:1px solid #cccccc;  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;"></td>
                                                            <td width="323" height="40" style="  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;"></td>
                                                        }
                                                        else
                                                        {
                                                            if (string.IsNullOrWhiteSpace(item.AccountClientLookupId))
                                                            {
                                                                <td width="323" height="40" style="border-right:1px solid #cccccc;  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;">@Model.Loc.Where(a => a.ResourceId == "spnManufacturer").FirstOrDefault().Value.ToString() : @item.Part_Manufacturer</td>
                                                                <td width="323" height="40" style="border-right:1px solid #cccccc;  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;"></td>
                                                                <td width="323" height="40" style="border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;">@Model.Loc.Where(a => a.ResourceId == "spnManufacturerID").FirstOrDefault().Value.ToString() : @item.Part_ManufacturerID</td>
                                                            }
                                                            else
                                                            {
                                                                <td width="323" height="40" style="border-right:1px solid #cccccc;  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;">@Model.Loc.Where(a => a.ResourceId == "spnAccount").FirstOrDefault().Value.ToString() : @item.AccountClientLookupId </td>
                                                                <td width="323" height="40" style="border-right:1px solid #cccccc;  border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;">@Model.Loc.Where(a => a.ResourceId == "spnManufacturer").FirstOrDefault().Value.ToString() : @item.Part_Manufacturer</td>
                                                                <td width="323" height="40" style="border-bottom:1px solid #cccccc; font-size:14px; color:#737272; line-height:40px; font-weight:normal; padding-left:15px;">@Model.Loc.Where(a => a.ResourceId == "spnManufacturerID").FirstOrDefault().Value.ToString() : @item.Part_ManufacturerID</td>
                                                            }

                                                        }
                                                    </tr>
                                                </table>
                                              
                                            </td>

                                        </tr>
                                    }
                                }
                            <tr>
                                <td height="40" colspan="4" align="center" valign="middle" style="border-right: 1px solid #cccccc; border-top: 1px solid #cccccc;"></td>
                                <td height="40" colspan="2" align="center" valign="middle" style="border-right: 1px solid #cccccc; border-top: 1px solid #cccccc; font-size: 14px; color: #000; line-height: 40px; font-weight: normal; font-weight: 500;">@Model.Loc.Where(a => a.ResourceId == "spnPoGrandtotal").FirstOrDefault().Value.ToString()</td>
                                <td align="center" valign="middle" height="40" style="font-size: 14px; color: #000; line-height: 40px; font-weight: 500; border-top: 1px solid #cccccc; border-right: 1px solid #cccccc;">@string.Format("{0:c2}", sum)</td>
                                <td height="40" align="center" valign="middle" style="border-right:1px solid #cccccc;"></td>
                                <td height="40" align="center" valign="middle"></td>
                            </tr>
                            </table>
                           
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        @if (!string.IsNullOrEmpty(Model.PurchaseOrderModel.MessageToVendor))
        {
            <tr>
                <td align="left" valign="top" height="10"><img src="../Content/images/space.png" alt="" /></td>
            </tr>
            <tr>
                <td align="left" valign="top" style="border:1px solid #b6b6b6; padding:15px;">
                    <h4 style="padding:0; margin:0 0 5px 0; font-size:15px; color:#222222; font-weight:500;">@Model.Loc.Where(a => a.ResourceId == "spnPoMessageToVendor").FirstOrDefault().Value.ToString() :</h4>
                    <p style="padding:0 0 5px 0; margin:0; font-size:14px; color:#5a5a5a; font-weight:normal; line-height:22px;">
                        @Model.PurchaseOrderModel.MessageToVendor
                    </p>
                </td>
            </tr>
        }
        <tr>
            <td align="left" valign="top" height="50"><img src="../Content/images/space.png" alt="" /></td>
        </tr>
       
    </table>
</body>
</html>
