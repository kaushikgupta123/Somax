@model Client.Models.PurchaseOrder.PurchaseOrderVM

<div id="requestDetailModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-info">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">@Model.Loc.Where(a => a.ResourceId == "spnPoRequestDetails").FirstOrDefault().Value.ToString()</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoRequest").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.ClientLookupId</label>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoCreatedBy").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.Creator_PersonnelName</label>
                        </div>
                    </div>
                    <div style="clear:both;"></div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoCreated").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.CreateDates</label>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoApprovedBy").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.Approved_PersonnelName</label>
                        </div>
                    </div>
                    <div style="clear:both;"></div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoApproved").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.ApprovedDate</label>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoProcessedBy").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.Processed_PersonnelName</label>
                        </div>
                    </div>
                    <div style="clear:both;"></div>

                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoProcessedDate").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Model.PurchaseOrderModel.purchaseRequestModel.ProcessedDate</label>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label for="Line"><strong>@Model.Loc.Where(a => a.ResourceId == "spnPoAutoGenerated").FirstOrDefault().Value.ToString() : </strong></label>
                            <label for="Line">@Html.CheckBoxFor(m => m.PurchaseOrderModel.purchaseRequestModel.AutoGenerated, new { @class = "readonly", @readonly = "readonly", onclick = "return false;" ,style= "position: relative;top: 2px;left: 6px;"})</label>
                        </div>
                    </div>
                    <div style="clear:both;"></div>
                </div>
            </div>
        </div>
    </div>
</div>
