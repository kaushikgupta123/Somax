@model Client.Models.PartTransfer.PartTransferVM

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<body style="padding:0; margin:0; font-family: 'Roboto', sans-serif; font-size:14px; color:#737272;">
    <table width="1000" border="0" cellspacing="0" cellpadding="0" align="center">
        <tr>
            <td width="557" height="120" align="left" valign="top" style="padding:0">
                 <a href="#"><img src="@Model.printPartTransferModel.AzureImageURL" alt="" style="height:157px; width:auto;" /></a>
            </td>
            <td width="108" height="125" align="left" valign="top" style="padding:0;">
                <img src='@Url.Content("~/Content/images/angleBg.jpg")' alt="" style="height:157px; width:108px;" />
            </td>
            <td width="310" height="95" align="left" valign="top" style="background:#e4effd; padding:25px 0 0 25px;">
                <h2 style="padding:0; margin:0 0 5px 0; font-size:28px; color:#264a7c; text-decoration:underline; font-weight:normal;">@Model.Loc.Where(a => a.ResourceId == "spnPartTransferShipment").FirstOrDefault().Value.ToString()</h2>
                <p style="padding:0; margin:0 0 7px 0; font-size:16px; color:#5d5d5d; font-weight:normal;">Transfer Number : <span style="color:#ff8400;">@Model.printPartTransferModel.PartTransferID</span></p>
            </td>
        </tr>
    </table>
</body>
</html>
