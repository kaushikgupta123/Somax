@Html.HiddenFor(m => m.PartModel.Section)
@Html.HiddenFor(m => m.PartModel.Row)
@Html.HiddenFor(m => m.PartModel.Shelf)
@Html.HiddenFor(m => m.PartModel.Bin)
@Html.HiddenFor(m => m.PartModel.Consignment)
