@model Client.Models.Parts.PartsVM
<div class="tabsArea nobg" id="NewVendors">
    <div class="m-portlet" id="m_portlet" style="padding:15px;">
        <div class="bottomBlock">
            <div class="table-responsive">
                <table id="pvendorTable_Mobile" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                    <thead>
                        <tr>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnVendors").FirstOrDefault().Value.ToString()</th>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnName").FirstOrDefault().Value.ToString()</th>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnCatalogNumber").FirstOrDefault().Value.ToString()</th>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnManufacturer").FirstOrDefault().Value.ToString()</th>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnManufacturerID").FirstOrDefault().Value.ToString()</th>
                            <th style="text-align:right !important">@Model.Loc.Where(a => a.ResourceId == "spnOrderQnt").FirstOrDefault().Value.ToString()</th>
                            <th>@Model.Loc.Where(a => a.ResourceId == "spnOrderUnit").FirstOrDefault().Value.ToString()</th>
                            <th style="text-align:right !important">@Model.Loc.Where(a => a.ResourceId == "spnPrice").FirstOrDefault().Value.ToString()</th>
                            <th style="text-align:right !important">@Model.Loc.Where(a => a.ResourceId == "IssueOrder").FirstOrDefault().Value.ToString()</th>
                            <th style="text-align:center !important">@Model.Loc.Where(a => a.ResourceId == "UOMConvRequired").FirstOrDefault().Value.ToString()</th>
                            <th width="17%" style="text-align:center !important;">@Model.Loc.Where(a => a.ResourceId == "globalAction").FirstOrDefault().Value.ToString()</th>
                        </tr>
                    </thead>

                </table>
            </div>
            <div class="bttn_pagin">
                <div class="bttns"><button type="button" class="btn btn-blue" data-toggle="" data-target="" id="btnAddpVendor_Mobile" style="display:none"><i class="fa fa-plus faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "btnAddVendor").FirstOrDefault().Value.ToString()</button></div>
                <div style="clear:both;"></div>
            </div>
            <div style="clear:both;"></div>
        </div>
    </div>
</div>