@model Client.Models.SensorAlert.SensorAlertVM
<div class="m-subheader">
  <div class="new-m-subheader">
    <div class="PageTitle">
      <div class="align-items-center separator">
        <h3 class="m-subheader__title ">
          @Model.Loc.Where(a => a.ResourceId == "spnSensorAlertProcedures").FirstOrDefault().Value.ToString()
        </h3>
      </div>
    </div>

    <div class="NewBreadcrumbArea">
      <div class="newBreadcrumb">
        <ul>
          <li><a href="/Dashboard/Dashboard">&nbsp;</a></li>
          <li class="active">@Model.Loc.Where(a => a.ResourceId == "spnSensorAlertProceduresSearch").FirstOrDefault().Value.ToString()</li>
        </ul>
        <div style="clear:both;"></div>
      </div>
    </div>

    <div class="ActionBttnArea">
      @if (Model.security.Sensors.AlertProcedures)
      {
        <button type="button" class="btn newBtn-add addsensoralert pull-left" title="Add Sensor Alert">@Model.Loc.Where(a => a.ResourceId == "spnAddSensorAlert").FirstOrDefault().Value.ToString()</button>
      }

      <div style="clear:both;"></div>
    </div>
    <div style="clear:both;"></div>
  </div>
</div>

<div class="row">
  <div class="col-xl-12">
    <div class="m-portlet" id="m_portlet" style="padding:15px; margin-bottom:0;">
      <div class="actionBar" style="display:none" id="SensorAlertGridAction">
        <div class="pull-left">
          <button type="button" class="btn btn-white navbar-btn" id="sidebarCollapse" title="Advance Search"><i class="fa fa-filter" style="font-size: 1.3rem;"></i></button>
          <ul class="filter_apply globalFilterAdjusted">
            <li class="textFilter"><span class="filteritemcount">0</span>&nbsp;@Model.Loc.Where(a => a.ResourceId == "globalFiltersAppiled").FirstOrDefault().Value.ToString()</li>
            <li class="textClear" id="liClearAdvSearchFilterSA"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "globalClear").FirstOrDefault().Value.ToString()</a></li>
          </ul>
          <div style="clear:both;"></div>
        </div>

        <div class="pull-right">
          <button type="button" class="btn btn-white import-export" disabled onclick="funcShowExportbtn()">@Model.Loc.Where(a => a.ResourceId == "spnExport").FirstOrDefault().Value.ToString()</button>
          <button type="button" id="liCustomize" class="btn btn-white" title="Customize" data-toggle="modal" data-target="#gridcolumncustomizemodal"><i class="fa fa-cog" style="font-size: 1.3rem;"></i></button>
          <div style="clear:both;"></div>
        </div>
        <div style="clear:both;"></div>
        <div id="advsearchfilteritems"></div>
        <div style="clear:both;"></div>
      </div>

      <div id="Active" class="tabcontent" style="display:block;">
        <div class="table-responsive">
          <table id="sensoralertSearch" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
            <thead>
              <tr>
                <th data-th-index="0">@Model.Loc.Where(a => a.ResourceId == "spnOnDemandID").FirstOrDefault().Value.ToString()</th>
                <th data-th-index="1">@Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString()</th>
                <th data-th-index="2">@Model.Loc.Where(a => a.ResourceId == "GlobalType").FirstOrDefault().Value.ToString()</th>
                <th data-th-index="3">@Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString()</th>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
@Html.Partial("_CustomizeGridSetup")
@Html.Partial("_Export")
@Html.Partial("~/Views/SensorAlert/_SensorAlertAdvSearch.cshtml")

