                    <li><a href="/Configuration/Dashboard">&nbsp;</a></li>
                    <li><a href="/ShipToAddress/Index?page=ShipTo_Master">@Model.Loc.Where(a => a.ResourceId == "globalSearchShipToAddress").FirstOrDefault().Value.ToString()</a></li>
                    @if (Model.shipToModel.ShipToId == 0)
                    {
                        <li class="active">@Model.Loc.Where(a => a.ResourceId == "globalAddShipToAddress").FirstOrDefault().Value.ToString()</li>
                    }
                    else
                    {
                        <li class="active">@Model.Loc.Where(a => a.ResourceId == "globalEditShipToAddress").FirstOrDefault().Value.ToString()</li>
                    }
                </ul>
                <div style="clear:both;"></div>
            </div>
        </div>
        <div style="clear:both;"></div>
    </div>
</div>
@Html.Partial("_ErrorDiv")
