                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalPassReqSpecialChar").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWRequireSpecialChar, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalSequentialRepeatCharNotAllowPassword").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWNoRepeatChar, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
