                                    <button class="tablinks" onclick="openCity(event, 'PRFormSettings')">@Model.Loc.Where(a => a.ResourceId == "spnPurchaseRequest").FirstOrDefault().Value.ToString()</button>
                                    <button class="tablinks" onclick="openCity(event, 'POFormSettings')">@Model.Loc.Where(a => a.ResourceId == "spnPurchaseOrder").FirstOrDefault().Value.ToString()</button>
                                    <button class="tablinks" onclick="openCity(event, 'PORFormSettings')">@Model.Loc.Where(a => a.ResourceId == "spnPurchaseOrderReceipt").FirstOrDefault().Value.ToString()</button>

