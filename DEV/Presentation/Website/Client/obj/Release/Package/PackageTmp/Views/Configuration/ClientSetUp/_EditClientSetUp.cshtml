                                            @Html.ValidationMessageFor(m => m.passwordSettingsModel.MaxAttempts, "", new { style = "display:none" })
                                            @Html.TextBoxFor(m => m.passwordSettingsModel.MaxAttempts, new { @class = "form-control search longinput", autocomplete = "off" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalReqMinlen").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWReqMinLength, new { @class = "form-control m-input", id = "PWReqMinLength", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                    </li>

                                    @if (Model.passwordSettingsModel.PWReqMinLength)
