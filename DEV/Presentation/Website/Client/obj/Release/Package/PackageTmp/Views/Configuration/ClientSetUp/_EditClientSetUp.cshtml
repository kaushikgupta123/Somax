                                                @Html.ValidationMessageFor(m => m.passwordSettingsModel.PWExpiresDays, "", new { style = "display:none" })
                                                @Html.TextBoxFor(m => m.passwordSettingsModel.PWExpiresDays, new { @class = "form-control search longinput", autocomplete = "off" })
                                            </span>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalPasswordAgeLimit").FirstOrDefault().Value.ToString(): <small style="display:none" id="PassAgeLimitReqId">*</small></span>
                                            <span class="inputText">
                                                @Html.ValidationMessageFor(m => m.passwordSettingsModel.PWExpiresDays, "", new { style = "display:none" })
                                                @Html.TextBox("passwordSettingsModel.PWExpiresDays", "", new { @class = "form-control search longinput readonly", @readonly = "readonly", autocomplete = "off", value = "" })
                                            </span>
                                        </li>
                                    }
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalpassReqNumber").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWRequireNumber, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalPassReqAlphaChar").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWRequireAlpha, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                    </li>

                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalBitPassMixedCase").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.PWRequireMixedCase, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
