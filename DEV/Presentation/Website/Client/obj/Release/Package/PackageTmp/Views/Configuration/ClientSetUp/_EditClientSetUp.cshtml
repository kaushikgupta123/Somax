                                            @Html.CheckBoxFor(m => m.passwordSettingsModel.AllowAdminReset, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                    </li>
                                    <div style="clear:both;"></div>
                                </ul>

                                <div class="bttn_pagin">
                                    <div class="bttns2">
                                        <button class="btn btn-blue mobBttn" name="Command" value="save" id="btnSavePS"><i class="fa fa-save faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalSave").FirstOrDefault().Value.ToString()</button>
                                    </div>
                                    <div style="clear:both;"></div>
                                </div>
                            </div>
                        }
                        @if (Model.security.WorkOrders.CompletionWizardConfiguration)
                        {
                            using (Ajax.BeginForm("UpdateWOCompletionSettings", "ClientSetUp", new AjaxOptions { HttpMethod = "Post", OnSuccess = "CompletionSettingsOnSuccess", OnBegin = "AjaxBeginFormBegin", OnComplete = "AjaxBeginFormComplete", OnFailure = "AjaxBeginFormFaillure" }))
