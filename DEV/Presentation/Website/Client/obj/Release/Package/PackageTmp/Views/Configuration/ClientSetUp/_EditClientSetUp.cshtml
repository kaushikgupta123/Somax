                                                <div class="row col-12">
                                                    <div class="row col-4 text-left">
                                                        @Html.CheckBoxFor(m => m.woCompletionSettingsModel.WOCompCriteriaTab, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
