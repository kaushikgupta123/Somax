@model Client.Models.Configuration.ClientSetUp.ClientSetUpVM
@{
    string imageUrl = Model.clientSetUpModel.ImageUrl;
    if (Model.clientSetUpModel.ClientOnPremise)
    {
        imageUrl = UtilityFunction.PhotoBase64ImgSrc(Model.clientSetUpModel.ImageUrl);
    }

}
<div class="m-subheader">
    <div class="new-m-subheader">
        <div class="PageTitle">
            <div class="align-items-center separator">
                <h3 class="m-subheader__title ">
                    @Model.Loc.Where(a => a.ResourceId == "spnConfiguration").FirstOrDefault().Value.ToString()
                </h3>
            </div>
        </div>
        <div class="NewBreadcrumbArea">
            <div class="newBreadcrumb">
                <ul>
                    <li><a href="/Configuration/Dashboard">&nbsp;</a></li>
                    <li class="active">@Model.Loc.Where(a => a.ResourceId == "spnClientSetup").FirstOrDefault().Value.ToString()</li>
                </ul>
                <div style="clear:both;"></div>
            </div>
        </div>
        <div style="clear:both;"></div>
    </div>
</div>

@Html.Partial("_ErrorDiv")

<div class="row">
    <div class="col-xl-3">
        <div class="m-portlet">
            <div class="sideMenuArea nobg">
                <ul class="vtabs" id="detmaintab">
                    <li class="active" id="idOverview"><a href="#Overview">@Model.Loc.Where(a => a.ResourceId == "spnOverview").FirstOrDefault().Value.ToString()</a></li>
                    <li id="photot"><a href="#Photos" id="anchPhoto">@Model.Loc.Where(a => a.ResourceId == "spnPhotos").FirstOrDefault().Value.ToString() </a></li>
                    <li id="Passwordsetting" onclick="openCity(event, 'Passwordsettings')"><a href="#Passwordsettings">@Model.Loc.Where(a => a.ResourceId == "GlobalPasswordSettings").FirstOrDefault().Value.ToString() </a></li>
                    @if (Model.security.WorkOrders.CompletionWizardConfiguration)
                    {
                        <li id="CompletionSetting" onclick="openCity(event, 'CompletionSettings')"><a href="#CompletionSettings">@Model.Loc.Where(a => a.ResourceId == "spnWOCompletionWizardSettings").FirstOrDefault().Value.ToString() </a></li>
                    }
                    @if (Model.security.WorkOrder_FormConfiguration.Access)
                    {
                        <li id="FormConfigurationSetting" onclick="openCity(event, 'FormConfigurationSettings')"><a href="#FormConfigurationSettings">@Model.Loc.Where(a => a.ResourceId == "spnWorkOrderFormConfiguration").FirstOrDefault().Value.ToString() </a></li>
                    }
                </ul>
            </div>
            <div class="selectTabs" id="selectTabs">
                <select class="form-control" id="colorselector">
                    <option value="idOverview">@Model.Loc.Where(a => a.ResourceId == "spnOverview").FirstOrDefault().Value.ToString()</option>
                    <option value="Photos">@Model.Loc.Where(a => a.ResourceId == "spnPhotos").FirstOrDefault().Value.ToString()</option>
                    <option value="Passwordsettings">@Model.Loc.Where(a => a.ResourceId == "GlobalPasswordSettings").FirstOrDefault().Value.ToString()</option>
                    @if (Model.security.WorkOrders.CompletionWizardConfiguration)
                    {
                        <option value="CompletionSettings">@Model.Loc.Where(a => a.ResourceId == "spnWOCompletionWizardSettings").FirstOrDefault().Value.ToString() </option>
                    }
                    @if (Model.security.WorkOrder_FormConfiguration.Access)
                    {
                        <option value="FormConfigurationSettings">@Model.Loc.Where(a => a.ResourceId == "spnWorkOrderFormConfiguration").FirstOrDefault().Value.ToString() </option>
                    }
                </select>
            </div>
        </div>
    </div>
    <div class="col-xl-9">
        <div class="row">
            <div class="col-xl-12" id="btnnblock">
                <div class="m-portlet" id="m_portlet" style="padding:15px;">
                    <div class="bottomBlock">
                        @using (Ajax.BeginForm("UpdateClientSetup", "ClientSetUp", new AjaxOptions { HttpMethod = "Post", OnSuccess = "ClientUpdateAddOnSuccess", OnBegin = "AjaxBeginFormBegin", OnComplete = "AjaxBeginFormComplete", OnFailure = "AjaxBeginFormFaillure" }))
                        {
                            @Html.AntiForgeryToken()
                            <div class="tabsArea nobg" id="Overview" style="display:block;">
                                <ul class="listView nobrd">
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientName").FirstOrDefault().Value.ToString(): <small>*</small></span>
                                        <span class="inputText">
                                            @Html.ValidationMessageFor(m => m.clientSetUpModel.CompanyName, "", new { style = "display:none" })
                                            @Html.TextBoxFor(m => m.clientSetUpModel.CompanyName, new { @class = "form-control search ", autocomplete = "off", maxlength = "63" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnPrimaryContact").FirstOrDefault().Value.ToString(): <small>*</small></span>
                                        <span class="inputText">
                                            @Html.ValidationMessageFor(m => m.clientSetUpModel.PrimaryContact, "", new { style = "display:none" })
                                            @Html.TextBoxFor(m => m.clientSetUpModel.PrimaryContact, new { @class = "form-control search ", autocomplete = "off", maxlength = "63" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnPhone").FirstOrDefault().Value.ToString(): </span>
                                        <span class="inputText">
                                            @Html.TextBoxFor(m => m.clientSetUpModel.OfficerPhone, new { @class = "form-control search ", autocomplete = "off", maxlength = "31" })
                                        </span>
                                    </li>
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnEmail").FirstOrDefault().Value.ToString():<small>*</small> </span>
                                        <span class="inputText">
                                            @Html.ValidationMessageFor(m => m.clientSetUpModel.Email, "", new { style = "display:none" })
                                            @Html.TextBoxFor(m => m.clientSetUpModel.Email, new { @class = "form-control search ", autocomplete = "off", maxlength = "255" })
                                        </span>
                                    </li>
                                    @if (!(Model.clientSetUpModel._userdata.Site.APM == false && Model.clientSetUpModel._userdata.Site.CMMS == false && Model.clientSetUpModel._userdata.Site.Sanitation == true) && !(Model.clientSetUpModel._userdata.Site.APM == true && Model.clientSetUpModel._userdata.Site.CMMS == false && Model.clientSetUpModel._userdata.Site.Sanitation == false))
                                    {
                                        <li style="height:auto;">
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnWorkOrderMessage").FirstOrDefault().Value.ToString():</span>
                                            <span class="inputText" style="height:auto;">
                                                @Html.TextAreaFor(m => m.clientSetUpModel.WOPrintMessage, new { @class = "form-control search", style = "resize:none;", autocomplete = "off", maxlength = "511" })
                                            </span>
                                            <div style="clear:both;"></div>
                                        </li>
                                    }
                                    @*commented for v2-516*@
                                    @*@if (Model.clientSetUpModel._userdata.DatabaseKey.Client.PackageLevel.ToUpper() == PackageLevelConstant.Enterprise.ToUpper())
        {*@
                                    <li style="height:auto;">
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnAssetTree").FirstOrDefault().Value.ToString():</span>
                                        <span class="inputText">
                                            @Html.CheckBoxFor(m => m.clientSetUpModel.AssetTree, new { @class = "form-control m-input", @style = "color:black; width: auto; margin-top: 12px;" })
                                        </span>
                                        <div style="clear:both;"></div>
                                    </li>
                                    @*}*@

                                    <li style="height:auto;">
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnPMWorkOrderGenerationMethod").FirstOrDefault().Value.ToString():</span>
                                        <span class="inputText">
                                            @Html.DropDownListFor(m => m.clientSetUpModel.PMWOGenerateMethod, Model.PMWOGenerationMethodList, "--Select--", new { @class = "form-control select2picker", style = "width:100%;" })
                                        </span>
                                        <div style="clear:both;"></div>
                                    </li>
                                    @if (Model.clientSetUpModel._userdata.Site.Sanitation)
                                    {
                                        <li style="height:auto;">
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spanMasterSanitationJobGenerationMethod").FirstOrDefault().Value.ToString():</span>
                                            <span class="inputText">
                                                @Html.DropDownListFor(m => m.clientSetUpModel.MasterSanGenerateMethod, Model.MasterSanGenerateMethodList, "--Select--", new { @class = "form-control select2picker", style = "width:100%;" })
                                            </span>
                                            <div style="clear:both;"></div>
                                        </li>
                                    }

                                </ul>
                                <div class="bttn_pagin">
                                    <div class="bttns2">
                                        <button class="btn btn-blue mobBttn" name="Command" value="save" id="btnSaveWO"><i class="fa fa-save faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalSave").FirstOrDefault().Value.ToString()</button>
                                    </div>
                                    <div style="clear:both;"></div>
                                </div>
                            </div>
                            @Html.HiddenFor(x => x.clientSetUpModel.UpdateIndex)

                            @Html.HiddenFor(m => m.clientSetUpModel.ClientId)
                        }
                        <div class="tabsArea nobg imageDropZone" id="Photos" style="display:none;">
                            <div class="row">
                                <div class="col-xl-4">
                                    <div id="AzureImage" class="AzureImage">

                                        @if (!string.IsNullOrEmpty(imageUrl))
                                        {
                                            <img src="@imageUrl" id="EquipZoom" class="equipImg" alt="Image" data-zoom-image="@imageUrl" />
                                        }
                                        else
                                        {
                                            <img src="~/Scripts/ImageZoom/images/NoImage.jpg" id="EquipZoom" class="equipImg" alt="Image" data-zoom-image="/Scripts/ImageZoom/images/NoImage.jpg" />
                                        }
                                        @if (!string.IsNullOrEmpty(Model.clientSetUpModel.ImageUrl) && !Model.clientSetUpModel.ImageUrl.Contains("NoImage"))
                                        {
                                            <a href="javascript:void(0)" id="deleteImg" class="trashIcon" title="@Model.Loc.Where(a => a.ResourceId == "globalDelete").FirstOrDefault().Value.ToString()"><i class="fa fa-trash"></i></a>
                                        }
                                    </div>
                                </div>
                                <div class="col-xl-8">
                                    <div class="hldDropzone">
                                        <div class="dropzone" id="dropzoneForm">
                                            <div class="fallback">
                                                <input name="file" type="file" multiple />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        @using (Ajax.BeginForm("UpdatePasswordSettings", "ClientSetUp", new AjaxOptions { HttpMethod = "Post", OnSuccess = "PasswordSettingsOnSuccess", OnBegin = "AjaxBeginFormBegin", OnComplete = "AjaxBeginFormComplete", OnFailure = "AjaxBeginFormFaillure" }))
                        {
                            @Html.AntiForgeryToken()
                            <div class="tabsArea nobg" id="Passwordsettings" style="display:none;">
                                <ul class="listView nobrd">
                                    <li>
                                        <span class="label">@Model.Loc.Where(a => a.ResourceId == "GlobalMaxFailLoginAttempts").FirstOrDefault().Value.ToString(): <small>*</small></span>
