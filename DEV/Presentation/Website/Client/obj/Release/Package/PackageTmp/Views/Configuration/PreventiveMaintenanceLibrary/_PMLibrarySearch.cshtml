@model Client.Models.Configuration.PreventiveMaintenanceLibrary.PrevMaintLibraryVM
<div class="m-subheader">
    <div class="new-m-subheader">
        <div class="PageTitle">
            <div class="align-items-center separator">
                <h3 class="m-subheader__title ">
                    @Model.Loc.Where(a => a.ResourceId == "spnLibraries").FirstOrDefault().Value.ToString()
                </h3>
            </div>
        </div>

        <div class="NewBreadcrumbArea">
            <div class="newBreadcrumb">
                <ul>
                    <li><a href="/Configuration/Dashboard">&nbsp;</a></li>
                    <li class="active">@Model.Loc.Where(a => a.ResourceId == "spnPMLibrarySearch").FirstOrDefault().Value.ToString() </li>
                </ul>
                <div style="clear:both;"></div>
            </div>
        </div>

        <div class="ActionBttnArea">
            @if (Model.security.PrevMaintLibrary.Create)
            {<button type="button" class="btn newBtn-add addPrevMaintLibrary pull-left" title="New Preventive Maintenance Library">@Model.Loc.Where(a => a.ResourceId == "spnNewPreventiveMaintenanceLibrary").FirstOrDefault().Value.ToString()</button>
        }

        </div>
        <div style="clear:both;"></div>
    </div>
</div>

<div class="row">
    <div class="col-xl-12">
        <div class="m-portlet" id="m_portlet" style="padding:15px;">
            <div class="actionBar" style="display:none" id="PMlibraryGridAction">
                <div class="pull-left spaceAdjust2">
                    <button type="button" class="btn btn-white navbar-btn" id="PMLsidebarCollapse" title="Advance Search"><i class="fa fa-filter" style="font-size: 1.3rem;"></i></button>
                    <ul class="filter_apply globalFilterAdjusted">
                        <li class="textFilter"><span class="filteritemcount">0</span>&nbsp;@Model.Loc.Where(a => a.ResourceId == "globalFiltersAppiled").FirstOrDefault().Value.ToString()</li>
                        <li class="textClear" id="liClearAdvSearchFilterPML"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "globalClear").FirstOrDefault().Value.ToString()</a></li>
                    </ul>
                    <div style="clear:both;"></div>
                </div>
                <div class="pull-right singleBttn2">
                    <button type="button" class="btn btn-white import-export"  onclick="funcShowExportbtn()">@Model.Loc.Where(a => a.ResourceId == "spnExport").FirstOrDefault().Value.ToString()</button>
                    <button type="button" class="btn btn-white" id="liCustomize" title="Customize" data-toggle="modal" data-target="#gridcolumncustomizemodal"><i class="fa fa-cog" style="font-size: 1.3rem;"></i></button>

                    <div style="clear:both;"></div>
                </div>
                <div style="clear:both;"></div>
                <div id="advsearchfilteritems"></div>
                <div style="clear:both;"></div>
            </div>
            <div style="clear:both;"></div>

            <div class="tabcontent" style="display:block;">
                <div class="table-responsive">
                    <table id="prevMaintLibrarySearchTable" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                        <thead>
                            <tr>
                                <th data-th-index="0">@Model.Loc.Where(a => a.ResourceId == "spnPMMasterID").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="1">@Model.Loc.Where(a => a.ResourceId == "spnDescription").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="2">@Model.Loc.Where(a => a.ResourceId == "spnPMDuration").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="3">@Model.Loc.Where(a => a.ResourceId == "spnFrequencyType").FirstOrDefault().Value.ToString()</th>
                                <th data-th-index="4">@Model.Loc.Where(a => a.ResourceId == "spnFrequency").FirstOrDefault().Value.ToString() </th>
                                <th data-th-index="5">@Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString()</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("_CustomizeGridSetup")
@Html.Partial("_Export")

<style type="text/css">
    #popupExport { right:97px }
    @@media only screen and (width:1360px)
    {
        #popupExport { right: 97px !important; }
    }
</style>

<div id="PMLibraryadvsearchcontainer">
    @Html.Partial("~/Views/Configuration/PreventiveMaintenanceLibrary/_PMLibraryAdvSearch.cshtml")
</div>


