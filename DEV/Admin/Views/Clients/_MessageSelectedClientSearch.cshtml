@model Admin.Models.Client.ClientVM

<div id="MessageSelectedClientMessageGridModal" class="modal fade" role="dialog">
    <div class="modal-dialog childrenPop" style="overflow:hidden;">
        <div class="overlay2"></div>
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close btncancelschedule" data-dismiss="modal">&times;</button>
                <div class="align-items-center">
                    <h3 class="m-subheader__title ">
                        @Model.Loc.Where(a => a.ResourceId == "spnMessageSelectedClient").FirstOrDefault().Value.ToString()
                    </h3>
                </div>
                <div style="clear:both;"></div>
            </div>

            <div class="modal-body">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="m-portlet" id="m_portlet" style="padding:15px; margin-bottom:0;">
                            <div class="table-responsive">
                                <table id="tblMessageSelectedClientList" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th style="text-align:center !important;">@Model.Loc.Where(a => a.ResourceId == "spnMessageID").FirstOrDefault().Value.ToString()</th>
                                            <th>@Model.Loc.Where(a => a.ResourceId == "spnMessage").FirstOrDefault().Value.ToString()</th>
                                            <th>@Model.Loc.Where(a => a.ResourceId == "globalStartDate").FirstOrDefault().Value.ToString()</th>
                                            <th>@Model.Loc.Where(a => a.ResourceId == "globalEndDate").FirstOrDefault().Value.ToString()</th>
                                            <th style="text-align:left !important;">@Model.Loc.Where(a => a.ResourceId == "globalCreateDate").FirstOrDefault().Value.ToString()</th>
                                            <th style="text-align:center !important;">@Model.Loc.Where(a => a.ResourceId == "globalAction").FirstOrDefault().Value.ToString()</th>
                                        </tr>
                                    </thead>

                                </table>
                            </div>
                            <div class="bttn_pagin">
                                <div class="bttns"><button type="button" class="btn btn-blue" data-toggle="" data-target="" id="btnAddClientMessage" style="display:none;"><i class="fa fa-plus faIcon"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalAdd").FirstOrDefault().Value.ToString()</button></div>
                                <div style="clear:both;"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>
<div id="AddEditMessageSelectedClientModalpopup" class="modal fade" role="dialog">
    <div class="modal-dialog" id="AddEditMessageSelectedClientModalDialog">
    </div>
</div>
