@model  Admin.Models.SupportTicket.SupportTicketVM
<nav id="sidebar">
    <div id="dismiss">
        <i class="fa fa-close"></i>
    </div>
    <div class="sidebar-header">
        <h3><i class="fa fa-filter" style="position: relative; top: -2px;"></i>&nbsp; @Model.Loc.Where(a => a.ResourceId == "globalFilter").FirstOrDefault().Value.ToString() <span id="spnControlCounter">0</span></h3>
        <button type="button" class="btn btn-blue filterSearch" id="btnClientDataAdvSrch"> @Model.Loc.Where(a => a.ResourceId == "GlobalApply").FirstOrDefault().Value.ToString()</button>
    </div>
    <div class="sidebar-content" id="advsearchsidebar">
        <form action="#" method="post">
            <div class="form-group">
                <label for="SupportTicketID" id="lblSupportTicketID" name="SupportTicketID">@Model.Loc.Where(a => a.ResourceId == "spnSupportTicketID").FirstOrDefault().Value.ToString() </label>
                <input type="text" class="form-control search adv-item" id="SupportTicketID" autocomplete="off" style="color:#333;">
            </div>
            <div class="form-group">
                <label for="Subject" id="lblSubject">@Model.Loc.Where(a => a.ResourceId == "globalSubject").FirstOrDefault().Value.ToString() </label>
                <input type="text" class="form-control search adv-item" id="Subject" autocomplete="off" style="color:#333;">
            </div>
            <div class="form-group">
                <label for="Contact" id="lblContact">@Model.Loc.Where(a => a.ResourceId == "spnContact").FirstOrDefault().Value.ToString() </label>
                <input type="text" class="form-control search adv-item" id="Contact" autocomplete="off" style="color:#333;">
            </div>
            <div class="form-group">
                <label for="Status" id="lblStatus">@Model.Loc.Where(a => a.ResourceId == "GlobalStatus").FirstOrDefault().Value.ToString()</label>
                <input type="text" class="form-control search adv-item" id="Status" autocomplete="off" style="color:#333;">
            </div>
            <div class="form-group">
                <label for="Agent" id="lblAgent">@Model.Loc.Where(a => a.ResourceId == "spnAgent").FirstOrDefault().Value.ToString() </label>
                <input type="text" class="form-control search adv-item" id="Agent" autocomplete="off" style="color:#333;">
            </div>
            <div class="form-group">
                <label for="Created">@Model.Loc.Where(a => a.ResourceId == "spnCreated").FirstOrDefault().Value.ToString() :</label>
                <input type="text" class="form-control search adv-item dtpicker" id="Created" autocomplete="off">
            </div>
            <div class="form-group">
                <label for="Completed">@Model.Loc.Where(a => a.ResourceId == "spnGlobalCompleted").FirstOrDefault().Value.ToString() :</label>
                <input type="text" class="form-control search adv-item dtpicker" id="Completed" autocomplete="off">
            </div>
        </form>
    </div>
</nav>

