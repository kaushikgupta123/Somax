                                            <span class="label">@Html.LabelFor(m => m.ClientModel.Name) :</span>
                                            <span class="inputText">@Model.ClientModel.Name</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.Contact) :</span>
                                            <span class="inputText">@Model.ClientModel.Contact</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.Email) :</span>
                                            <span class="inputText">@Model.ClientModel.Email</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.BusinessType) :</span>
                                            <span class="inputText">@Model.ClientModel.BusinessType</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.PackageLevel) :</span>
                                            <span class="inputText">@Model.ClientModel.PackageLevel</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.Status) :</span>
                                            <span class="inputText">@Model.ClientModel.Status</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "globalCreateDate").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">
