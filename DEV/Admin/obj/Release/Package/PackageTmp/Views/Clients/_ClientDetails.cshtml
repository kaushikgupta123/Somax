@model Admin.Models.Client.ClientVM

<link href="~/Scripts/Metronic/assets/demo/default/base/style_newCustom.css" rel="stylesheet" />


<div class="m-subheader fixedDiv1">
    <div class="new-m-subheader">
            
        <div class="filterDescription">
            <a href="javascript:void(0)" id="linkToSearch"><span id="spnlinkToSearch"></span></a>
                
            - @Model.ClientModel.ClientId
        </div>

        <div class="ActionBttnArea">
            <div class="actionDiv pull-left" id="ad1">
                <div class="btn-group bootstrap-select m-datatable__pager-size dropup show selectDrop actionNewDrop" style="margin-right:0;">
                    <div class="btn-group bootstrap-select m-datatable__pager-size" style="width: 100%;">
                        <button type="button" class="btn dropdown-toggle bs-placeholder btn-default" data-toggle="dropdown" role="button" title=""><span class="filter-option pull-left">@Model.Loc.Where(a => a.ResourceId == "globalAction").FirstOrDefault().Value.ToString()</span>&nbsp;<span class="bs-caret"><span class="caret"></span></span></button>
                        <div class="dropdown-menu open" role="combobox">
                            <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                @if (Model.ClientModel.Status == "Inactive")
                                {
                                    <li id="actinctivateclient"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnGlobalActivate").FirstOrDefault().Value.ToString()</a></li>
                                }
                                @if (Model.ClientModel.Status == "Active")
                                {
                                    <li id="actinctivateclient"><a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnGlobalInactivate").FirstOrDefault().Value.ToString()</a></li>
                                }
                            </ul>
                            <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                <li id="actionSiteMaintainence">
                                    <a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnWebSiteMaintenanceMessage").FirstOrDefault().Value.ToString() </a>
                                </li>
                            </ul>
                            <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                <li id="actionMessageSelectedClient">
                                    <a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnMessageSelectedClient").FirstOrDefault().Value.ToString() </a>
                                </li>
                            </ul>
                            <ul class="dropdown-menu inner" role="listbox" aria-expanded="false">
                                <li id="actionMessageAllClients">
                                    <a href="javascript:void(0)">@Model.Loc.Where(a => a.ResourceId == "spnMessageAllClients").FirstOrDefault().Value.ToString() </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <button type="button" class="btn newBtn-add AddClient pull-left" id="AddClient" title="Add Client "><i class="fa fa-plus"></i></button>

            <div style="clear:both;"></div>
        </div>
        
    </div>
</div>

<div class="pushDiv1" style="height:76px !important;"></div>
<div class="row">
    <div class="col-xl-2">
        <div class="m-portlet">
            <div class="sideMenuArea nobg">
                <ul class="vtabs" id="detmaintab">
                    <li class="tablinks active" id="clientSidebar" onclick="openCity(event, 'ClientOverview')"><a href="#ClientOverview">@Model.Loc.Where(a => a.ResourceId == "globalOverview").FirstOrDefault().Value.ToString()</a></li>

                    <li id="clientsites" class="tablinks" onclick="openCity(event, 'ClientSites')"><a href="#ClientSites">@Model.Loc.Where(a => a.ResourceId == "spnSites").FirstOrDefault().Value.ToString()</a></li>

                </ul>
            </div>

            <div class="selectTabs" id="selectTabs">
                <select class="form-control" id="colorselector">
                    <option value="ClientOverview">@Model.Loc.Where(a => a.ResourceId == "globalOverview").FirstOrDefault().Value.ToString()</option>

                    <option value="ClientSites">@Model.Loc.Where(a => a.ResourceId == "spnSites").FirstOrDefault().Value.ToString()</option>

                </select>
            </div>
        </div>
    </div>

    <div class="col-xl-10">
        <div class="row">
