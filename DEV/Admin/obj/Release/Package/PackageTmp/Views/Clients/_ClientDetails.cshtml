    <div style="clear:both;"></div>
    <div class="col-xl-12">
        <div class="tabsArea nobg" id="ClientActivity" style="margin-top:20px;">
            <div class="m-portlet" id="m_portlet" style="padding:15px; margin-bottom:0;">
                <div class="m-portlet__head" style="padding:0; height:2.5rem; margin-bottom:15px;">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <h3 class="m-portlet__head-text" style="vertical-align:top;">
                                @Model.Loc.Where(a => a.ResourceId == "GlobalActivity").FirstOrDefault().Value.ToString()
                            </h3>
                        </div>
                    </div>
                    <div style="clear:both;"></div>
                </div>
                
                <div style="clear:both;"></div>
                <div class="table-responsive">
                    <table id="ActivityTable" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                        <thead>
                            <tr>
                                <th class="CreateDate">@Model.Loc.Where(a => a.ResourceId == "GlobalDate").FirstOrDefault().Value.ToString()</th>
                                <th class="UserInfoId">@Model.Loc.Where(a => a.ResourceId == "spnGlobalUserInfoID").FirstOrDefault().Value.ToString()</th>
