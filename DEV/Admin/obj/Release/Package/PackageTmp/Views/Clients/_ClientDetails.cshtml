                    </div>
                    
                    <div style="clear:both;"></div>
                </div>
                
                <div style="clear:both;"></div>
                <div class="table-responsive">
                    <table id="ClientEventLogTable" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                        <thead>
                            <tr>
                                <th class="TransactionDate">@Model.Loc.Where(a => a.ResourceId == "spnDate").FirstOrDefault().Value.ToString()</th>
                                <th class="EventlogId">@Model.Loc.Where(a => a.ResourceId == "spnEventID").FirstOrDefault().Value.ToString()</th>
