                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.SiteControl) :</span>
                                            <span class="inputText">@Html.CheckBoxFor(m => m.ClientModel.SiteControl, new { @class = "form-control m-input", onclick = "return false;", @style = "color:black; width: auto; margin-top: 12px;" })</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientAppUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.AppUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientMaxAppUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.MaxAppUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientWorkRequestUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.WorkRequestUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientMaxWorkRequestUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.MaxWorkRequestUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientSanitationUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.SanitationUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientMaxSanitationUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.MaxSanitationUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientProductionUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.ProdAppUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnClientMaxProductionUsers").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.MaxProdAppUsers</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "spnSites").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.Sites</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.MaxSites) :</span>
                                            <span class="inputText">@Model.ClientModel.MaxSites</span>
                                        </li>
                                        <li>
                                            <span class="label">@Model.Loc.Where(a => a.ResourceId == "globalLocalisation").FirstOrDefault().Value.ToString() :</span>
                                            <span class="inputText">@Model.ClientModel.Localization</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.MaxAttempts) :</span>
                                            <span class="inputText">@Model.ClientModel.MaxAttempts</span>
                                        </li>
                                        <li>
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.MaxTimeOut) :</span>
                                            <span class="inputText">@Model.ClientModel.MaxTimeOut</span>
                                        </li>
                                        <li class="content-col">
                                            <span class="label">@Html.LabelFor(m => m.ClientModel.ConnectionString) :</span>
                                            <span class="inputText" style="height: 40px !important; overflow: scroll !important;">@Model.ClientModel.ConnectionString</span>
                                        </li>
                                    </ul>
                                </div>
                                <div class="editBttn">
                                    <button type="button" id="btnclientedit" class="btn btn-blue"><i class="fa fa-pencil" style="margin-top:-3px;"></i> &nbsp; @Model.Loc.Where(a => a.ResourceId == "globalEdit").FirstOrDefault().Value.ToString()</button>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>



                <div class="tabsArea nobg" id="ClientSites" style="display:none;">
                    <div class="m-portlet" id="m_portlet" style="padding:10px;">
                        <div class="table-responsive">
                            <table id="ClientSiteTable" class="eqpDtlTbl row-border custom-sort" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "globalSiteID").FirstOrDefault().Value.ToString()</th>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "spnSiteNameGlobal").FirstOrDefault().Value.ToString()</th>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "spnStatus").FirstOrDefault().Value.ToString()</th>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "GlobalAPM").FirstOrDefault().Value.ToString()</th>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "GlobalCMMS").FirstOrDefault().Value.ToString()</th>
                                        <th>@Model.Loc.Where(a => a.ResourceId == "spnGlobalSanitation").FirstOrDefault().Value.ToString()</th>
